<% for (let i=0; i < votes.length; i++) {%>
  <li class="match__list">
    <span class="user__vote" hidden><%=matches[i].userVote%></span>
    <span class="match__date" hidden><%=matches[i].gameStartDate%></span>
      
    <div class="left__team">
      <img src="/images/<%=matches[i].homeTeamCode%>.png" alt="team logo" />
      <span class="team__name"><%=matches[i].homeTeamName%></span>
      <span class="votes"><%=matches[i].homeTeamVotes%></span>
    </div>
    <!-- <span class="vs">VS</span> -->
  <span class="vs">날짜: <%=matches[i].gameStartDate%> 획득 포인트: <%=votes[i].point%></span>
    <div class="right__team">
      <span class="votes"><%=matches[i].awayTeamVotes%></span>
      <span class="team__name"><%=matches[i].awayTeamName%></span>
      <img src="/images/<%=matches[i].awayTeamCode%>.png" alt="team logo" />
    </div>
  </li>
<%}%>